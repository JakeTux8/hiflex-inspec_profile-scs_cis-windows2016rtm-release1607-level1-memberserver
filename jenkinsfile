node {
   stage('Staging') {
      // changing to build directory
      sh 'cd /tmp/build'
      // Getting profile from a GitHub repository
      git 'https://github.com/JakeTux8/hiflex-inspec_profile-scs_cis-windows2016rtm-release1607-level1-memberserver.git'
   }
   stage('Build') {
      //ar
      sh 'inspec compliance login https://chef-automate.example.com --user admin --ent="ei" --insecure --dctoken "93a49a4f2482c64126f7b6015e6b0f30284287ee4054ff8807fb63d9cbd1c506"'
      sh 'inspec compliance upload /tmp/build/hiflex-inspec_profile-scs_cis-windows2016rtm-release1607-level1-memberserver'
      sh 'rm -rf /tmp/build/*'
   }
   stage('Results') {
      echo 'Build Complete'
   }
}
